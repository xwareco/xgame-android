<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uencom.xgame.xgame"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="19" />

    <meta-data android:name="com.google.android.gms.version" />

    <supports-screens android:requiresSmallestWidthDp="600" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="uencom.xgame.demos.DemosLauncher" >
        </activity>
        <activity android:name="uencom.xgame.demos.SpeechandSoundDemo" />
        <activity android:name="uencom.xgame.demos.GesturesDemo" />
        <activity android:name="uencom.xgame.demos.SensorsDemo" />

        <service android:name="uencom.xgame.sensors.AddressFetcher" />

        <activity android:name="uencom.xgame.demos.TTSDemo" />
        <activity android:name="uencom.xgame.engine.xGameParser" />
        <activity android:name="uencom.xgame.demos.Splash" />
        <activity android:name="uencom.xgame.engine.MainView" />
        <activity
            android:name="uencom.xgame.engine.DownlodView"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity android:name="uencom.xgame.engine.GameView" />
        <activity android:name="uencom.xgame.engine.GameOver" />
        <activity android:name="uencom.xgame.engine.LoginOrRegister" />
        <activity android:name="uencom.xgame.engine.Login" />
        <activity android:name="uencom.xgame.engine.Register" />
        <activity android:name="uencom.xgame.engine.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>